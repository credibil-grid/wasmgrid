    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "http-kv",
            "cargo": {
                "args": [
                    "build",
                    "--bin=wasmgrid",
                    "--package=wasmgrid"
                ],
                "filter": {
                    "name": "wasmgrid",
                    "kind": "bin"
                }
            },
            "args": [
                "./target/wasm32-wasi/release/http_kv.wasm"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "http",
            "cargo": {
                "args": [
                    "build",
                    "--bin=wasmgrid",
                    "--package=wasmgrid"
                ],
                "filter": {
                    "name": "wasmgrid",
                    "kind": "bin"
                }
            },
            "args": [
                "./target/wasm32-wasi/release/http.wasm"
            ],
            "cwd": "${workspaceFolder}"
        },
    ]
}