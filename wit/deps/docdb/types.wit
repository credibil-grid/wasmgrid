interface types {
    /// A database connection.
    resource database {
        connect: static func(name: string) -> result<database, error>;
    }

    resource statement {
        prepare: static func(collection: string, filter: list<tuple<string, string>>) -> result<statement, error>;
    }

    /// An error resource type.
    resource error {
		trace: func() -> string;
  	}
}